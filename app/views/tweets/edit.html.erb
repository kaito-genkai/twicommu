<div class="text-center mb-4">
  <h1>ツイート編集</h1>
</div>

<%= form_with(model: @tweet, local: true) do |f| %>
  
  <div class="form-group">
    <%=f.label :content,"ツイート内容 : " %><br>
    <%= f.text_field :content, class: "form-control" %>
  </div>  
  
  <%= f.submit "変更", class:"btn btn-primary btn-block" %>
  
<% end %>