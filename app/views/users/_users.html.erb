<% if users.any? %>
  <ul class="list-unstyled">
    <% users.each do |user| %>
      <li class="media">
        <img class="mr-2 rounded" src="<%= user.image %>" alt="">
        <div class="media-body">
          <div>
            <%= user.name %>
          </div>
          <div>
            <p><%= link_to "ユーザ詳細", user_path(user) %></p>
          </div>
        </div>
      </li>
    <% end %>  
  </ul>
  <%= paginate users %>
<% end %>  
  